{
    "name": "gustjoha/flarum-swedish",
    "type": "flarum-extension",
    "description": "Swedish language extension for flarum.",
    "keywords": ["extension", "flarum", "language", "locale", "swedish", "translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gustav Johansson",
            "email": "gust.joha@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/gustjoha/flarum-swedish",
        "source": "https://github.com/gustjoha/flarum-swedish"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "extra": {
        "flarum-extension": {
            "title": "Swedish for Flarum",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "sv",
            "title": "Svenska"
        }
    }
}
